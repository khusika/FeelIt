{
  "name": "feelit-src",
  "version": "1.0.3",
  "description": "FeelIt theme source file",
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "babel-preset-minify": "^0.5.2",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "core-js": "^3.30.2",
    "husky": "^8.0.3",
    "minimist": "^1.2.8",
    "mkdirp": "^3.0.1",
    "segmentit": "^2.0.3"
  },
  "scripts": {
    "build": "rm -f -r exampleSite/public && hugo --source=exampleSite --gc",
    "build-lunr-segmentit": "browserify src/lib/lunr.segmentit.js -o assets/lib/lunr/lunr.segmentit.js -t babelify --presets @babel/preset-env --presets minify",
    "purgejs": "npx mkdirp exampleSite/assets/js && sed '652,656d;674,723d;726,729d;815d' assets/js/theme.js > exampleSite/assets/js/theme.js",
    "deploy": "npm run purgejs && hugo --source=exampleSite --gc --minify && rm -rf exampleSite/assets/js",
    "start": "hugo server --source=exampleSite -D --disableFastRender",
    "start-production": "hugo server --source=exampleSite -D --disableFastRender -e production",
    "copy": "rm -rf resources && mv -f exampleSite/resources resources",
    "reinstall": "rm -rf node_modules && npm ci",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khusika/FeelIt.git"
  },
  "keywords": [
    "hugo",
    "theme"
  ],
  "author": "Khusika",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khusika/FeelIt/issues"
  },
  "homepage": "https://github.com/khusika/FeelIt#readme"
}
